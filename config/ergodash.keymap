                <&kp KP_NUMBER_4>,
                <&macro_release>,
                <&kp KP_NUMBER_4>,
                <&macro_press>,
                <&kp KP_N1>,
                <&macro_release>,
                <&kp KP_N1 &kp LEFT_ALT>;

