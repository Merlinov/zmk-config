        left_bracket: left_bracket {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&macro_press>,
                <&kp LEFT_ALT>,
                <&macro_press>,
                <&kp KP_N9>,
                <&macro_release>,
                <&kp KP_N9>,
                <&macro_press>,
                <&kp KP_NUMBER_1>,
                <&macro_release>,
                <&kp KP_N1 &kp LEFT_ALT>;

            label = "LEFT_BRACKET";
        };

        right_bracket: right_bracket {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&macro_press>,
                <&kp LEFT_ALT>,
                <&macro_press>,
                <&kp KP_NUMBER_9>,
                <&macro_release>,
                <&kp KP_N9>,
                <&macro_press>,
                <&kp KP_N3>,
                <&macro_release>,
                <&kp KP_N3 &kp LEFT_ALT>;

