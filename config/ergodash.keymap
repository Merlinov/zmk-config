                <&kp KP_N1>,
                <&macro_release>,
                <&kp KP_N1>,
                <&macro_press>,
                <&kp KP_N2>,
                <&macro_release>,
                <&kp KP_NUMBER_2>,
                <&macro_press>,
                <&kp KP_NUMBER_3>,
                <&macro_release>,
                <&kp KP_NUMBER_3 &kp LEFT_ALT>;

