                <&kp LEFT_ALT>,
                <&macro_press>,
                <&kp KP_NUMBER_3>,
                <&macro_release>,
                <&kp KP_NUMBER_3>,
                <&macro_press>,
                <&kp KP_NUMBER_3>,
                <&macro_release>,
                <&kp KP_NUMBER_3 &kp LEFT_ALT>;

